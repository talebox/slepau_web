{
	"extends": [
		"@parcel/config-default"
	],
	"resolvers": ["parcel-resolver-ignore", "..."],
	"transformers": {
		"*.svelte": [
			"parcel-transformer-svelte-v4"
		],
		"raw:*": [
			"@parcel/transformer-raw"
		],
		"*.md": [
			"parcel-transformer-markdown"
		]
	},
	"compressors": {
		"*.{html,css,js,svg,db}": [
			"...",
			"@parcel/compressor-brotli"
		]
	}
}